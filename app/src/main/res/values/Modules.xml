<?xml version="1.0" encoding="utf-8"?>
<!--
    This file holds the array of available modules and an integer for the number of modules
    modCount is used to dynamically loop through many array (particularly in LayoutConfig.Java) so it must be kept current
-->
<resources xmlns:tools="http://schemas.android.com/tools">
    <string-array name="greeting" tools:keep="@array/greeting">
        <item>Good Morning!</item>
    </string-array>
    <string name="weather" tools:keep="@string/weather">Weather</string>
    <string name="time" tools:keep="@string/time">Time</string>
    <string name="news" tools:keep="@string/news">News</string>
    <string name="email" tools:keep="@string/email">Email</string>
    <integer name="modCount">6</integer>
</resources>